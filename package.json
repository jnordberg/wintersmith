{
  "name": "wintersmith",
  "version": "1.1.1",
  "author": "Johan Nordberg <its@johan-nordberg.com>",
  "description": "A flexible static site generator.",
  "license": "MIT",
  "main": "./lib/",
  "keywords": [
    "static",
    "generator",
    "markdown",
    "jade"
  ],
  "bin": {
    "wintersmith": "./bin/wintersmith"
  },
  "scripts": {
    "prepublish": "./bin/dev/compile_coffee"
  },
  "directories": {
    "lib": "./lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jnordberg/wintersmith.git"
  },
  "dependencies": {
    "marked": "git://github.com/jnordberg/marked.git",
    "coffee-script": "1.3.x",
    "async": "0.1.x",
    "highlight": "0.2.x",
    "jade": ">=0.25.x",
    "ncp": "0.2.x",
    "rimraf": "2.x.x",
    "winston": "0.6.x",
    "colors": "0.x.x",
    "glob": "3.1.x",
    "optimist": "0.3.x",
    "underscore": "1.3.x",
    "minimatch": "0.2.x",
    "mime": "1.2.x",
    "moment": "1.7.x"
  },
  "engine": {
    "node": ">=0.6"
  }
}
