    "optimist": "~0.6.0",
    "minimatch": "~0.2.14",
    "mime": "~1.2.11",
    "js-yaml": "~3.0.1",
    "mkdirp": "~0.3.5",
    "chokidar": "~1.0.0",
    "server-destroy": "~1.0.0",
