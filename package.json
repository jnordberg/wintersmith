{
  "name": "wintersmith",
  "version": "2.0.2",
  "author": "Johan Nordberg <its@johan-nordberg.com>",
  "description": "A flexible static site generator.",
  "license": "MIT",
  "main": "./lib/",
  "keywords": [
    "static",
    "generator",
    "markdown",
    "jade"
  ],
  "bin": {
    "wintersmith": "./bin/wintersmith"
  },
  "scripts": {
    "prepublish": "node ./bin/dev/compile_coffee"
  },
  "directories": {
    "lib": "./lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jnordberg/wintersmith.git"
  },
  "dependencies": {
    "marked": "0.2.x",
    "coffee-script": ">=1.3.0",
    "async": "0.2.x",
    "highlight.js": "7.x.x",
    "jade": ">=0.25.0",
    "ncp": "0.2.x",
    "rimraf": "2.x.x",
    "winston": "0.6.x",
    "colors": "0.x.x",
    "glob": "3.1.x",
    "optimist": "0.3.x",
    "minimatch": "0.2.x",
    "mime": "1.2.x",
    "js-yaml": "1.0.x",
    "mkdirp": "0.3.x",
    "chokidar": "0.6.x",
    "server-destroy": "1.0.x",
    "npm": "1.2.x"
  },
  "devDependencies": {
    "shelljs": "0.1.x"
  },
  "engine": {
    "node": ">=0.6"
  }
